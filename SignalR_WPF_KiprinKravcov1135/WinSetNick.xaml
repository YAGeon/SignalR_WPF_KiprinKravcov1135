<Window x:Class="SignalR_WPF_KiprinKravcov1135.WinSetNick"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SignalR_WPF_KiprinKravcov1135"
        mc:Ignorable="d"
        Title="Регистрация" Height="150" Width="200">
    <StackPanel>
        <TextBlock Text="{Binding Message}"/>
        <TextBox Text="{Binding Nick}"/>
        <Button Content="Отправить" Click="Send"/>
    </StackPanel>
</Window>
